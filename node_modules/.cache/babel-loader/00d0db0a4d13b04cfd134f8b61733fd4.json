\t\tif(e.response.status == 401) {\n\t                  this.$store.dispatch('logout')\n\t                  .then(() => {\n\t                    let path = window.location.href\n\t                    path = path.split('/')\n\t                    localStorage.setItem('prevPath', path[path.length - 1])\n\t                    this.$swal('Sesi login kamu sudah habis', 'login kagi yah...', 'warning')\n\t                    setTimeout(() => {\n\t                    \tthis.$swal.close()\n\t                    \tthis.$router.replace({path: '/login'})\n\t                    }, 1500)\n\t                    \n\t                  })\n\t                  .catch(e => {\n\t                    this.$swal('Tidak bisa ambil data', 'hubungi pengembangnya...', 'danger')\n\t                    setTimeout(() => {\n\t                    \tthis.$swal.close()\n\t                    }, 1500)\n\t                    return false\n\t                  })\n\t                }\n\t                else {\n\t                  \n\t                  console.log(e)\n\t                  this.$swal('Tidak bisa ambil data', 'hubungi pengembangnya...', 'danger')\n\t                    setTimeout(() => {\n\t                    \tthis.$swal.close()\n\t                    }, 1500)\n\t                  return false \n\t                }\n\t\t\t\t})\n\t\t\t}\n\t\t},\n\t\tcreated() {\n\t\t\tif(localStorage.level != 1 && localStorage.level != 2 && localStorage.level != 3) {\n\t\t\t\tthis.$router.push('/')\n\t\t\t}\n\t\t\tthis.getData()\n\t\t}\n\t}\n</script>"],"sourceRoot":"src/views/kwitansi"}]}