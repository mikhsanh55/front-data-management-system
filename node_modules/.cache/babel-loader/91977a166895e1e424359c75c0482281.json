uter.replace({path: '/login'})\r\n\t\t\t                  })\r\n\t\t\t                  .catch(e => {\r\n\t\t\t                    this.$swal('Ada sedikit error, harap hubungi pengembang softwarenya yah :)', 'Mohon tunggu sebentar...', 'error')\r\n\t\t\t                    return false\r\n\t\t\t                  })\r\n\t\t\t                }\r\n\t\t\t                else {\r\n\t\t\t                  console.log(e)\r\n\t\t\t                  this.$swal('Ada sedikit error, harap hubungi pengembang softwarenya yah :)', 'Mohon tunggu sebentar...', 'error')\r\n\t\t\t                  return false \r\n\t\t\t                }\r\n\t\t\t\t\t\t\treturn false\r\n\t\t\t\t\t\t})\r\n\t\t\t\t\t}\r\n\t\t\t\t\telse {\r\n\t\t\t\t\t\tthis.$swal.close()\r\n\t\t\t\t\t}\r\n\t\t\t\t})\r\n\t\t\t\t\t\t\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tif(localStorage.level != 1 && localStorage.level != 2 && localStorage.level != 4 ) {\r\n\t\t\t\tthis.$router.push('/')\r\n\t\t\t}\r\n\t\t\tthis.getData()\r\n\t\t}\r\n\t}\r\n</script>\r\n<style type=\"text/css\">\r\n\t#stockinout-table {\r\n\t\twidth: 100%;\r\n\t}\r\n\t#stockinout-table .form-inline label {\r\n\t\tdisplay: none;\t\r\n\t}\t\r\n\t#stockinout-table .form-inline input[type=text] {\r\n\t\twidth: 130%;\r\n\t}\r\n\t#stockinout-table .form-inline {\r\n\t\t\r\n\t\tdisplay: flex;\r\n\t\twidth: 100%;\r\n\t\tjustify-content: flex-start;\r\n\t}\r\n\t.VuePagination {\r\n\t\tmargin-top: 20px;\t\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: center;\r\n\t}\r\n</style>"],"sourceRoot":"src/views/produk"}]}