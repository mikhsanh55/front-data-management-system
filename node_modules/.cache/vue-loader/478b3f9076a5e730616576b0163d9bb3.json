tItem('prevPath', path[path.length - 1])\r\n\t                    this.$swal('Sesi login kamu sudah habis', 'login kagi yah...', 'warning')\r\n\t                    setTimeout(() => {\r\n\t                    \tthis.$swal.close()\r\n\t                    \tthis.$router.replace({path: '/login'})\r\n\t                    }, 1500)\r\n\t                    \r\n\t                  })\r\n\t                  .catch(e => {\r\n\t                    this.$swal('Tidak bisa ambil data', 'hubungi pengembangnya...', 'danger')\r\n\t                    setTimeout(() => {\r\n\t                    \tthis.$swal.close()\r\n\t                    \tthis.$router.replace({path: '/login'})\r\n\t                    }, 1500)\r\n\t                    return false\r\n\t                  })\r\n\t                }\r\n\t                else {\r\n\t                  this.label = 'Update Barang'\r\n\t                  this.$swal('Tidak bisa ambil data', 'hubungi pengembangnya...', 'danger')\r\n\t                    setTimeout(() => {\r\n\t                    \tthis.$swal.close()\r\n\t                    \tthis.$router.replace({path: '/login'})\r\n\t                    }, 1500)\r\n\t                    return false\r\n\t                }\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\tcheckImage(src) {\r\n\t\t\t\tlet image = new Image()\r\n\t\t\t\timage.onload = () => {\r\n\t\t\t\t\tconsole.log('ada')\r\n\t\t\t\t\tthis.image = 1\r\n\t\t\t\t\treturn true\r\n\t\t\t\t}\r\n\t\t\t\timage.onerror = () => {\r\n\t\t\t\t\tconsole.log('tidak ada')\r\n\t\t\t\t return false}\r\n\t\t\t\timage.src = src\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tthis.getData()\r\n\t\t}\r\n\t}\r\n</script>"]}]}