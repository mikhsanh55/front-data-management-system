rrors.length) {\r\n\t\t\t\t\tthis.$http.post('http://localhost:3000/kurir', this.kurir)\r\n\t\t\t\t\t.then(() => {\r\n\t\t\t\t\t\tthis.$router.push('/kurir')\r\n\t\t\t\t\t})\r\n\t\t\t\t\t.catch(e => {\r\n\t\t\t\t\t\tconsole.log(e)\r\n\t\t\t\t\t\treturn false\r\n\t\t\t\t\t})\r\n\t\t\t\t}\r\n\t\t\t\telse {\r\n\t\t\t\t\twindow.scrollBy({ \r\n\t\t\t\t\t  top: -500, // could be negative value\r\n\t\t\t\t\t  left: 0, \r\n\t\t\t\t\t  behavior: 'smooth' \r\n\t\t\t\t\t})\r\n\t\t\t\t\treturn false\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tif(localStorage.level != 1 && localStorage.level != 2 ) {\r\n\t\t\t\tthis.$router.push('/')\r\n\t\t\t}\r\n\t\t\tthis.getData()\r\n\t\t}\r\n\t}\r\n</script>"]}]}